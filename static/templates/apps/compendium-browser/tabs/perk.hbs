<li class="item flexrow" data-entry-uuid="{{entry.uuid}}" data-type="Item" data-tooltip="{{entry.name}}">
    <div class="image">
        <img src="{{entry.img}}" alt="{{entry.name}}" loading="lazy" />
    </div>
    <div class="name">
        <a class="item-link">{{entry.name}}</a>
    </div>
    {{#if entry.cost}}
    <div class="cost">
      AP Cost: <b>{{entry.cost}}</b>
    </div>
    {{/if}}
</li>
